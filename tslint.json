{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "typedef": [
      false,
      "parameter",
      "property-declaration"
    ],
    "member-access": false,
    "no-any": false,
    "no-debugger": false,
    "no-console": false,
    "ordered-imports": false,
    "max-classes-per-file": false,
    "no-shadowed-variable": false,
    "jsx-no-lambda": false
  }
}